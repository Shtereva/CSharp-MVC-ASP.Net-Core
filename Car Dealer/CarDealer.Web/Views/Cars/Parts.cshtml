@model List<PartViewModel>

<div class="row">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <td>Name</td>
            <td>Price</td>
        </tr>
        </thead>
        <tbody>
        @foreach (var part in Model)
        {
            <tr>
                <td>@part.Name</td>
                <td>@part.Price</td>
            </tr>
        }
        </tbody>
    </table>
</div>

